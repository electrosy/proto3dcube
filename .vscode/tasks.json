{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "g++ Linux debug",
            "command": "/usr/bin/g++",
            "args": [
                "-std=gnu++1z",
                "-g",
                "${workspaceFolder}/proto3dcube.cpp",
                "${workspaceFolder}/Video.cpp",
                "${workspaceFolder}/Input.cpp",
                "${workspaceFolder}/Cube.cpp",
                "${workspaceFolder}/Transform.cpp",
                "${workspaceFolder}/Model.cpp",
                "${workspaceFolder}/Timer.cpp",
                "${workspaceFolder}/Renderable.cpp",
                "${workspaceFolder}/Clock.cpp",
                "${workspaceFolder}/src/ProgressBar.cpp",
                "${workspaceFolder}/src/RectContainer.cpp",
                "${workspaceFolder}/src/Vec3.cpp",
                
                "-l",
                "SDL2",
                "-o",
                "${workspaceFolder}/proto3dcube"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
       
    ],
    "version": "2.0.0"
}